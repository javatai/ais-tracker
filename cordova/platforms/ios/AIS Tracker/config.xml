<?xml version='1.0' encoding='utf-8'?>
<widget id="de.3epnm.aistracker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="CDVCertificate">
        <param name="ios-package" value="CDVCertificate" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="res/ios/icon-60.png" width="180" />
    <icon height="60" src="res/ios/icon-60.png" width="60" />
    <icon height="120" src="res/ios/icon-60.png" width="120" />
    <icon height="76" src="res/ios/icon-72.png" width="76" />
    <icon height="152" src="res/ios/icon-72@2x.png" width="152" />
    <icon height="40" src="res/ios/icon.png" width="40" />
    <icon height="80" src="res/ios/icon.png" width="80" />
    <icon height="57" src="res/ios/icon.png" width="57" />
    <icon height="114" src="res/ios/icon@2x.png" width="114" />
    <icon height="72" src="res/ios/icon-72.png" width="72" />
    <icon height="144" src="res/ios/icon-72@2x.png" width="144" />
    <icon height="29" src="res/ios/icon.png" width="29" />
    <icon height="58" src="res/ios/icon@2x.png" width="58" />
    <icon height="50" src="res/ios/icon-50.png" width="50" />
    <icon height="100" src="res/ios/icon-50@2x.png" width="100" />
    <splash height="480" src="res/ios/Default-Portrait.png" width="320" />
    <splash height="960" src="res/ios/Default-Portrait@2x.png" width="640" />
    <splash height="1136" src="res/ios/Default-568h@2x.png" width="640" />
    <splash height="1334" src="res/ios/Default-667h@2x.png" width="750" />
    <splash height="2208" src="res/ios/Default-Portrait-736h@3x.png" width="1242" />
    <name>AIS Tracker</name>
    <description>
        View AIS messages received in Hamburg
    </description>
    <author email="marcel.bretschneider@gmail.com" href="http://mb.aquarius.uberspace.de">
        Marcel Bretschneider
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
