    "url": "https://github.com/3epnm/ais-server/issues"
  },
  "homepage": "https://github.com/3epnm/ais-tracker#readme",
  "dependencies": {
    "backbone": "^1.2.3",
    "backbone-relational": "^0.10.0",
    "bootstrap-switch": "^3.3.2",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "file-stream-rotator": "0.0.6",
    "geographiclib": "^1.45.0",
    "jquery": "^2.1.4",
    "js-base64": "^2.1.9",
    "mapbox-gl-js": "git://github.com/mapbox/mapbox-gl-js.git",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "mysql": "^2.9.0",
    "nouislider": "leongersen/noUiSlider",
    "q": "^1.4.1",
    "redis": "^2.4.2",
    "sequelize": "^3.9.0",
    "socket.io": "^1.3.7",
    "sqlite3": "^3.1.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "brfs": "^1.4.1",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "envify": "^3.4.0",
    "less": "^2.5.3",
    "minifier": "^0.7.1",
    "spritezero-cli": "^1.0.1",
    "uglify-js": "^2.5.0",
    "watchify": "^3.4.0"
  },
  "scripts": {
    "build-js": "node_modules/browserify/bin/cmd.js -t envify app/index.js > html/index.js",
    "watch-js": "node_modules/watchify/bin/cmd.js app/index.js -o html/index.js -dv"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "bootstrap-notify": "./app/contrib/bootstrap.notify-3.1.5.js",
    "jquery-color": "./app/contrib/jquery.color-2.1.2.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "bootstrap-notify": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "bootstrap-switch": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery-color": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
