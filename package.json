  "repository": {
    "type": "git",
    "url": "git+https://github.com/3epnm/ais-server.git"
  },
  "author": "Marcel Bretschneider <marcel.bretschneider@gmail.com> (http://mb.aquarius.uberspace.de/)",
  "license": "MIT",
